    },
    "issues": {
      "uri": "https://github.com/hnuguse/yamlw_vststask/issues"
    },
    "support": {
      "uri": "h.nuguse@nipo.com"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/hnuguse/yamlw_vststask"
  },
  "files": [
    {
      "path": "YamlWriter"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
