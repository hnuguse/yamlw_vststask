{
  "manifestVersion": 1,
  "id": "vsts-yaml-writer",
  "publisher": "hnuguse",
  "name": "Custom Yaml Writer",
  "version": "0.0.1",
  "description": "Feed in parameters or file to write out yaml to new or existing files",
  "tags": [
    "Extension",
    "Marketplace",
    "Publish",
    "Package",
    "Install",
    "Continuous Integration",
    "Continuous Delivery",
